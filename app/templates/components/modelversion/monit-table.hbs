  <p>模型详情表</p>
   <table class="table table-bordered">
      <thead>
        <tr>
          <th>id</th>
          <th>变量名</th>
          <th>周期</th>
          <th>阈值</th>
          <th>数据开始时间</th>
          <th>数据结束时间</th>
          <th>创建时间</th>
        </tr>
      </thead>
      <tbody>
        {{#each tabledata as |t|}}
        <tr>
          <td>{{t.id}}</td>
          <td>{{t.field_name}}</td>

          <td>{{t.monit_period}}</td>
          <td>{{t.model_thd}}</td>
          <td>{{t.start_time}}</td>
          <td>{{t.end_time}}</td>
          <td>{{t.monit_time}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>