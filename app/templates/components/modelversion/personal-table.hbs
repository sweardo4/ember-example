  <p>模型详情表</p>
   <table class="table table-bordered">
      <thead>
        <tr>
          <th>id</th>
          <th>变量英文名</th>
          <th>变量中文名</th>
          <th>个性化阈值</th>
          <th>标准阈值</th>
          <th>数据开始时间</th>
          <th>数据结束时间</th>
        </tr>
      </thead>
      <tbody>
        {{#each tabledata as |t|}}
        <tr>
          <td>{{t.id}}</td>
          <td>{{t.field_name}}</td>
          <td>{{t.personal_zname}}</td>
          <td>{{t.model_thd}}</td>
          <td>{{t.standard_thd}}</td>
          <td>{{t.start_time}}</td>
          <td>{{t.end_time}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>